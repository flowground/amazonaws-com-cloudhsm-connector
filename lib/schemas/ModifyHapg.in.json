{
    "type": "object",
    "properties": {
        "Action": {
            "required": true,
            "type": "string"
        },
        "Version": {
            "required": true,
            "type": "string"
        },
        "requestBody": {
            "properties": {
                "HapgArn": {
                    "description": "The ARN of the high-availability partition group to modify.",
                    "pattern": "arn:aws(-iso)?:cloudhsm:[a-zA-Z0-9\\-]*:[0-9]{12}:hapg-[0-9a-f]{8}",
                    "type": "string"
                },
                "Label": {
                    "description": "The new label for the high-availability partition group.",
                    "pattern": "[a-zA-Z0-9_.-]{1,64}",
                    "type": "string"
                },
                "PartitionSerialList": {
                    "description": "The list of partition serial numbers to make members of the high-availability partition group.",
                    "items": {
                        "pattern": "\\d{6,12}",
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "required": [
                "HapgArn"
            ],
            "type": "object"
        },
        "X_Amz_Content_Sha256": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Date": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Algorithm": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Credential": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Security_Token": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Signature": {
            "required": false,
            "type": "string"
        },
        "X_Amz_SignedHeaders": {
            "required": false,
            "type": "string"
        }
    }
}